{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "outDir": "dist/",
        "lib": [
            "es2015", "es2017"
        ]
    },
    "exclude": [
        "node_modules",
        "lib"
    ],
   "angularCompilerOptions": {
     "strictMetadataEmit": true
   }
}